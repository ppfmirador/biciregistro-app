# This file is an example. Create a .env.local for staging and .env.production for production.
# DO NOT COMMIT .env files to version control.

# Firebase Configuration
NEXT_PUBLIC_FIREBASE_API_KEY="your-api-key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="your-project-id.firebaseapp.com"
NEXT_PUBLIC_FIREBASE_PROJECT_ID="your-project-id"
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET="your-project-id.appspot.com"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="your-sender-id"
NEXT_PUBLIC_FIREBASE_APP_ID="your-web-app-id"

# Optional: Firebase App Check Debug Token for local development
# Get this from the browser console on first run when running locally.
NEXT_PUBLIC_FIREBASE_APPCHECK_DEBUG_TOKEN="your-app-check-debug-token"
